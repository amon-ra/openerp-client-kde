<?xml version="1.0"?>

<terp>
	<data>
		<!-- 
		FTS will always have exactly 4 different priorities, as defined
		by PostgreSQL 
		-->
		<record model="fts.priority" id="A">
			<field name="name">A</field>
			<field name="value">100</field>
		</record>
		<record model="fts.priority" id="B">
			<field name="name">B</field>
			<field name="value">40</field>
		</record>
		<record model="fts.priority" id="C">
			<field name="name">C</field>
			<field name="value">20</field>
		</record>
		<record model="fts.priority" id="D">
			<field name="name">D</field>
			<field name="value">10</field>
		</record>

		<!-- FTS default configuration will always be available -->
		<record model="fts.configuration" id="default">
			<field name="name">default</field>
		</record>
	</data>
</terp>

